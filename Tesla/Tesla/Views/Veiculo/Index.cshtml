@model IEnumerable<Tesla.Models.Veiculo>

@{
    ViewBag.Title = "Index";
}

<h2>Veiculo</h2>

<p>
    @Html.ActionLink("Criar Novo", "Criar")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.VEIDESCRICAO)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.VEIVALOR)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.VEICODMODELO)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.VEIDESCRICAO)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.VEIVALOR)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.VEICODMODELO)
        </td>
        <td>
            @Html.ActionLink("Editar", "Edit", new { id=item.VEID }) |
            @Html.ActionLink("Detalhes", "Details", new { id=item.VEID }) |
            @Html.ActionLink("Excluir", "Delete", new { id=item.VEID })
        </td>
    </tr>
}

</table>
